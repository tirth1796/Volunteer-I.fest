<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tirthshah.volunteerifest">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ifest_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".CheckRegistration" />
        <activity android:name=".Details" />
        <activity android:name=".ShowRegistration" />
        <activity android:name=".ShowCollections" />
        <activity
            android:name=".Cancellation"
            android:label="@string/title_activity_cancellation"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>